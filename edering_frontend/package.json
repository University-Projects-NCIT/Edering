{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "watch:dev": "run-p watch:css dev:*",
    "watch:css": "postcss src/assets/styles.pcss -o public/global.css -w",
    "dev:css": "postcss src/assets/styles.pcss -o public/global.css",
    "dev": "run-p dev:*",
    "dev:routify": "routify",
    "dev:rollup": "rollup -c -w ",
    "build": "rollup -c",
    "start": "sirv public --single --host 0.0.0.0",
    "lint": "eslint . --ext .ts",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "watch": {
    "chokidar": false
  },
